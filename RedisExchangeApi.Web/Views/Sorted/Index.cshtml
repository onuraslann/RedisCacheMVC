@model HashSet<string>

@{
    ViewData["Title"] = "Index";
}
<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit">Add</button>
</form>

@foreach (var item in Model)
{

    <h2>@item <a asp-controller="Sorted" asp-action="Delete" asp-route-name="@item">Delete</a> </h2>
}
